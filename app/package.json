{
    "name": "component-oriented-web-app",
    "version": "1.0.0",
    "author": "nmrmasakazu",
    "private": true,
    "scripts": {
        "dev": "NODE_ENV=dev ts-node src/server/app.ts",
        "build:client": "next build src/client",
        "build:server": "tsc --project ./src/server/tsconfig.server.json",
        "build": "concurrently \"yarn build:server\" \"yarn build:client\"",
        "start": "NODE_ENV=production node src/server/dist/app.js"
    },
    "dependencies": {
        "axios": "^0.21.0",
        "express": "^4.16.4",
        "js-cookie": "^2.2.1",
        "jwt-decode": "^2.2.0",
        "next": "^8.0.4",
        "next-cookies": "^2.0.3",
        "next-redux-wrapper": "^3.0.0-alpha.2",
        "node-fetch": "^2.6.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.0.1",
        "redux": "^4.0.1",
        "styled-components": "^4.2.0",
        "styled-jsx-plugin-sass": "^1.0.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.8",
        "@types/js-cookie": "^2.2.6",
        "@types/jwt-decode": "^3.1.0",
        "@types/next": "^8.0.3",
        "@types/next-redux-wrapper": "^2.0.2",
        "@types/react": "^16.8.13",
        "@types/react-dom": "^16.8.3",
        "@types/react-redux": "^7.0.6",
        "@types/redux": "^3.6.0",
        "@types/styled-components": "^4.1.14",
        "@types/styled-jsx": "^2.2.8",
        "@types/uuid": "^3.4.4",
        "@zeit/next-typescript": "^1.1.1",
        "concurrently": "^5.3.0",
        "node-sass": "^5.0.0",
        "prettier": "^1.16.4",
        "redux-devtools-extension": "^2.13.8",
        "styled-jsx": "^3.3.2",
        "ts-node": "^8.0.3",
        "typescript": "^3.4.3"
    }
}
