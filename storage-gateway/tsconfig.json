{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "noUnusedParameters": false,
        "strict": true,
        "esModuleInterop": true,
        "outDir": "dist",
        "noEmit": false,
        "skipLibCheck": true,
        "types": [
            "node"
          ]
    }
}
